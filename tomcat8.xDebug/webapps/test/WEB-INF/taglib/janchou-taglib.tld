<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib
        PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
        "http://java.sun.com/j2ee/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>
  <tlib-version>1.0</tlib-version>
  <jsp-version>1.2</jsp-version>
  <short-name>myprefix</short-name>
  <uri>http://cn.java.janchou/jsp/core</uri>
  <description>
    	标签解析库例子
  </description>
  <tag>
    <name>echo</name>
    <tag-class>cn.java.tag.EchoTag</tag-class>
    <description> echo method </description>
    <attribute>
       <name>str</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  <tag>
    <name>date</name>
    <tag-class>cn.java.tag.DateTag</tag-class>
    <description> date method </description>
    <attribute>
       <name>timestamp</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>format</name>
       <required>true</required>
    </attribute>
    <attribute>
       <name>timeZone</name>
       <required>false</required>
    </attribute>
  </tag>
  <!-- 
  <tag-file>
      <name>tag1</name>
      <path>/WEB-INF/tags/tag1.tag</path>
  </tag-file> 
  -->
</taglib>
